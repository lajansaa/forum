<%= form_for current_user, :url => user_path(current_user) do |f| %>
	<p><%= f.label :first_name, "First Name" %></p>
    <p><%= f.text_field :first_name %></p>
    <p><%= f.label :last_name, "Last Name" %></p>
    <p><%= f.text_field :last_name %></p>
    <p><%= f.label :email, "Email" %></p>
    <p><%= f.text_field :email %></p>
    <p><%= f.label :address, "Address" %></p>
    <p><%= f.text_field :address %></p>
    <p><%= f.label :gender, "Gender" %></p>
    <p><%= f.text_field :gender %></p>
    <p><%= f.label :number, "Number" %></p>
    <p><%= f.text_field :number %></p>
	<p><%= f.submit "Update" %></p>
<% end %>
